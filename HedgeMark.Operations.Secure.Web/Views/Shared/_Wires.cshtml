                </div>

                <div class="panel panel-default" id="wireSourceModuleDetails" ng-show="WireSourceModuleDetails.IsSourceAvailable">
                    <div class="panel-heading" ng-click="collapseContainer('SourceModule')" data-toggle="collapse" data-target="#wireSourceDiv" aria-expanded="false" aria-controls="wireSourceDiv">
                        <span class="">Wire Source : {{ WireSourceModuleDetails.SourceModuleName }}</span>
                        <span class="glyphicon  pull-right" ng-class="{ 'glyphicon-chevron-right': !isWireSourceCollapsed , 'glyphicon-chevron-down': isWireSourceCollapsed }"></span>
                    </div>
                    <div class="panel-body collapse" id="wireSourceDiv" style="padding: 10px 15px 0px 15px; overflow-y: scroll; max-height: 270px; margin-bottom: 2px;">
                        <span id="spnWireSource">
                            <p class="messgeBlock" ng-show="WireSourceModuleDetails.AttachmentName!=''">
                                File Attachment:<a ng-click="fnDownloadWireSourceFile(WireSourceModuleDetails.SourceModuleName,WireSourceModuleDetails.AttachmentName,WireSourceModuleDetails.FileSource)">{{WireSourceModuleDetails.AttachmentName}}</a>
                            </p>
                            <p ng-repeat="(key, value) in WireSourceModuleDetails.Details" class="messgeBlock">
                                {{key}}:{{value}}
                            </p>
                            <p></p>
                        </span>
                    </div>
                </div>


                <div class="panel panel-default" id="wireWorkFlowLogs" ng-show="WireTicket.hmsWireWorkflowLogs.length > 0">
                    <div class="panel-heading" ng-click="collapseContainer('Workflow')" data-toggle="collapse" data-target="#wireWorkflowLogsDiv" aria-expanded="false" aria-controls="wireWorkflowLogsDiv">
                        <span class="">Workflow Logs {{ " (" + WireTicket.hmsWireWorkflowLogs.length + ")" }}</span>
                        <span class="glyphicon  pull-right" ng-class="{ 'glyphicon-chevron-right': !isWorkflowLogsCollapsed , 'glyphicon-chevron-down': isWorkflowLogsCollapsed }"></span>
                    </div>
                    <div class="panel-body collapse" id="wireWorkflowLogsDiv" style="padding: 10px 15px 0px 15px; overflow-y: scroll; max-height: 270px; margin-bottom: 2px;">
                        <span id="spnWireLogs">
                            <ul class="milestones  milestones-bordered">
                                <li ng-repeat="wireLog in WireTicket.hmsWireWorkflowLogs">
                                    <i class="milestone-default glyphicon {{getWireLogStatusIcon(wireLog, $index)}}"></i>
                                    <div class="panel panel-default">
                                        <div class="panel-heading panel-arrow" style="text-transform: initial; font-size: 12px; border-top: 2px !important; padding: 5px 10px;">
                                            <span> {{ workflowUsers[$index] + " " }} <span id="workflowStatus_{{$index}}" style="font-size:13px;"><b> {{ getWireLogStatus(wireLog, $index) }} </b></span> {{ getWireLogText(wireLog) }}</span>
                                        </div>
                                        <div class="panel-body" ng-show="wireLog.Comment.length > 0" style="padding: 6px 10px;">{{ wireLog.Comment }}</div>
                                    </div>
                                </li>
                            </ul>
                        </span>
                    </div>
                </div>

                <div class="panel panel-default ng-show-hide" id="wireSwiftMessages" data-toggle="collapse" ng-show="IsSwiftMessagesPresent">
                    <div class="panel-body" style="cursor: pointer;padding:0px 15px 5px 15px;" ng-click="fnToggleCollapeSwiftMessagePanel()">
                        <span style="margin-top: 20px;position: fixed;text-transform: uppercase;">
                            Swift Messages :
                            &nbsp;<button class="btn btn-default btn-xs ng-show-hide" ng-hide="SwiftFormatMessageActiveTag==''" ng-click="fnPrintSwiftMessage($event)"><i class="glyphicon glyphicon-print"></i>&nbsp;Print</button>
                            &nbsp;<button class="btn btn-default btn-xs ng-show-hide" ng-hide="SwiftFormatMessageActiveTag==''" ng-click="fnCopySwiftMessageToClipboard($event)"><i class="glyphicon glyphicon-copy"></i>&nbsp;Copy to Clipboard</button>
                        </span>
                        <ol class="status-track" style="text-align: center;    padding-bottom: 0px;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    ">
                            <li class="{{value.OriginalFinMsg ==''?'status-track-todo': key.indexOf('N-Ack')>=0 ? 'status-track-failed' : 'status-track-done'}}" ng-class="{'status-active': SwiftFormatMessageActiveTag==key}" ng-repeat="(key, value) in wireTicketObj.SwiftMessages" ng-click="fnShowFormattedSwiftMsg($event,key,value)">{{key}}</li>
                        </ol>
                        <span class="glyphicon  pull-right" style="top:-15px" ng-class="{ 'glyphicon-chevron-right': !isSwiftMessagesCollapsed , 'glyphicon-chevron-down': isSwiftMessagesCollapsed }"></span>
                    </div>
                    <div class="panel-body collapse" id="wireSwiftMessagesDiv" style="padding: 2px 15px 0px 15px; overflow-y: scroll; max-height: 270px; margin-bottom: 2px;">
                        <p class="ng-show-hide swiftMessgeBlock originalFINMessage" ng-animate-swap="TrustedSwiftFINMessage">{{TrustedSwiftFINMessage}}</p>
                        <p class="ng-show-hide swiftMessgeBlock" ng-animate-swap="TrustedSwiftMessage">{{TrustedSwiftMessage}}</p>
                    </div>
                </div>
            </div>

            <div class="modal-footer form-inline">
                <div class="panel panel-heading collapse wireError" id="wireErrorStatus" data-toggle="collapse" style="background: #F5F5F5;text-transform: inherit; font-size:13px;">
                    <p style="color: darkred; font-weight:bold;"> {{ validationMsg }}</p>
                    <div class="active" data-toggle="buttons" id="wireCreationDiv" ng-show="isWireCreated">
                        <input id="chkWireCreation" type="checkbox" data-width="60" data-height="30" data-onstyle="btn btn-sm btn-success" data-offstyle="btn btn-sm btn-default" data-toggle="toggle" data-on="Yes" data-off="No">
                        &nbsp;<span>I understand and wish to initiate this wire.</span>
                    </div>
                </div>
                <textarea type="text" placeholder="Enter Comments" id="wireComment" ng-model="wireComments" class="form-control" rows="2" style="width:50%;float:left;"></textarea>
                <button id="draftWire" type="button" class="btn btn-sm btn-warning ng-show-only" title="Save" ng-disabled="isMandatoryFieldsMissing || !isWireRequirementsFilled || !isValidWireInitiation || isUserActionDone" ng-if="WireTicketStatus.IsDraftEnabled" ng-click="fnUpdateWireWithStatus(1)" style="vertical-align:-webkit-baseline-middle;" data-loading-text="<i class='glyphicon glyphicon-refresh icon-rotate'></i>&nbsp;<span class='statusFill'>{{ WireTicketStatus.IsLastModifiedUser ?  'Modifying' : 'Reverting to Draft' }}...</span>">
                    <i class="glyphicon glyphicon-floppy-save"></i>&nbsp;{{ WireTicketStatus.IsLastModifiedUser ? "Modify" : "Save as Draft" }}
                </button>
                <button id="initiateWire" type="button" class="btn btn-sm btn-primary ng-show-only" title="Save" ng-disabled="isMandatoryFieldsMissing || !isWireRequirementsFilled || !isValidWireInitiation || isUserActionDone" ng-if="WireTicketStatus.IsEditEnabled" ng-click="fnUpdateWireWithStatus(2)" style="vertical-align:-webkit-baseline-middle;" data-loading-text="<i class='glyphicon glyphicon-refresh icon-rotate'></i>&nbsp;<span class='statusFill'>Initiating...</span>">
                    <i class="glyphicon glyphicon-open"></i>&nbsp;{{ getInitiateButtonText() }}
                </button>
                <button id="holdWire" type="button" class="btn btn-sm btn-info ng-show-only" title="Save" ng-if="WireTicketStatus.IsWireStatusInitiated" ng-click="fnUpdateWireWithStatus(6)" style="vertical-align:-webkit-baseline-middle;" data-loading-text="<i class='glyphicon glyphicon-refresh icon-rotate'></i>&nbsp;<span class='statusFill'>Holding...</span>">
                    <i class="glyphicon glyphicon-ban-circle"></i>&nbsp;Hold
                </button>
                <button id="unHoldWire" type="button" class="btn btn-sm btn-info ng-show-only" title="Save" ng-if="WireTicketStatus.IsWireStatusOnHold" ng-click="fnUpdateWireWithStatus(2)" style="vertical-align:-webkit-baseline-middle;" data-loading-text="<i class='glyphicon glyphicon-refresh icon-rotate'></i>&nbsp;<span class='statusFill'>Un holding...</span>">
                    <i class="glyphicon glyphicon-exclamation-sign"></i>&nbsp;Un-Hold and {{getInitiateButtonText()}}
                </button>
                <button id="approveWire" type="button" class="btn btn-sm btn-success ng-show-only" title="Approve" ng-disabled="isUserActionDone" ng-if="WireTicketStatus.IsAuthorizedUserToApprove" ng-click="fnUpdateWireWithStatus(3)" style="vertical-align:-webkit-baseline-middle;" data-loading-text="<i class='glyphicon glyphicon-refresh icon-rotate'></i>&nbsp;<span class='statusFill'>Approving...</span>">
                    <i class="glyphicon glyphicon-ok-sign"></i>&nbsp;Approve
                </button>
                <button id="cancelWire" type="button" class="btn btn-sm btn-info ng-show-only cancelWire" ng-disabled="isUserActionDone" ng-if="WireTicketStatus.IsCancelEnabled" ng-click="confirmCancellation()" style="vertical-align:-webkit-baseline-middle;" data-loading-text="<i class='glyphicon glyphicon-refresh icon-rotate'></i>&nbsp;<span class='statusFill'>{{ getCancelButtonText(true) }}...</span>">
                    <i class="glyphicon glyphicon-repeat"></i>&nbsp;{{ getCancelButtonText(false) }}
                </button>
                <a href="#" class="btn btn-sm btn-default" style="vertical-align:-webkit-baseline-middle;" data-dismiss="modal">Dismiss</a>
                <br />
            </div>
        </div>
    </div>
</div>
