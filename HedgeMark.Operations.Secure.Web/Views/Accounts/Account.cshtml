@using HMOSecureWeb.Utility
@{
    ViewBag.Title = "OnBoarding - Account";
}

<legend>OnBoarding - Accounts</legend>

<div ng-controller="AccountCtrl">
    <div class="container">
        <input type="hidden" id="userName" value='@User.Identity.Name' />
        <div class="row">
            <div id="accountOnBoardContainer">
                <form name="accountForm">
                    <div class="panel panel-default container-fluid" style="position: inherit;" data-toggle="collapse" id="basicAccountDetail">
                        <div class="panel-heading row" style="cursor: pointer;">
                            <h4 class="panel-title">Basic Details</h4>
                        </div>
                        <div id="basicDetailCP" class="panel-collapse collapse in">
                            <div class="col-sm-8 col-md-8 col-lg-8 col-lg-8">
                                <div class="row control-group" style="margin-left: 10px; margin-right: 10px;">
                                    <br />
                                    <span class="row">
                                        <span class="col-sm-4 col-md-3 col-lg-3">
                                            <label class="control-label">Entity Type <span style="color: red">  *</span></label>
                                        </span>
                                        <span class="col-sm-8 col-md-8 col-lg-8">
                                            <input name="Account Type" type="text" class="form-control" id="liAccountType" required />
                                        </span>
                                    </span>
                                    <span class="row">
                                        <span class="col-sm-4 col-md-3 col-lg-3">
                                            <label class="control-label">Fund <span style="color: red">  *</span></label>
                                        </span>
                                        <span class="col-sm-8 col-md-8 col-lg-8">
                                            <input name="Fund" type="text" class="form-control" id="liFund" required />
                                        </span>
                                    </span>
                                    <span class="row" style="display: none;" id="spnBroker">
                                        <span class="col-sm-4 col-md-3 col-lg-3">
                                            <label class="control-label">Broker <span style="color: red">  *</span></label>
                                        </span>
                                        <span class="col-sm-8 col-md-8 col-lg-8">
                                            <input name="Broker" type="text" class="form-control" id="liBroker" required />
                                        </span>
                                    </span>
                                    <span class="row" style="display: none;" id="spnAgreement">
                                        <span class="col-sm-4 col-md-3 col-lg-3">
                                            <label class="control-label">Agreement <span style="color: red">  *</span></label>
                                        </span>
                                        <span class="col-sm-8 col-md-8 col-lg-8">
                                            <input name="Agreement" type="text" class="form-control" id="liAgreement" required />
                                        </span>
                                    </span>
                                    <br />
                                </div>
                            </div>
                        </div>
                    </div>

                    <div id="plnAddAccount" class="well well-sm" style="display: none;">
                        <a class="btn btn-primary" ng-click="fnAddAccountDetail()"><i class="glyphicon glyphicon-plus"></i>&nbsp; Add Account Detail</a>&nbsp;&nbsp;
                        <span id="agrName" class="label label-default shadowBox" style="padding-top: 6px; padding-bottom: 6px; text-transform: capitalize; display: none; font-size: 12px;"></span>
                        @*<span class="pull-right" style=" margin-left: 20px !important;"><input type="text"sticky-header class="live-search-box htFilterSearch form-control input-sm" placeholder="search here" aria-describedby="inputSuccess3Status" /></span>*@
                        <span class="pull-right">
                            <a id="btnAgrExpandAllPanel" class="btn btn-sm btn-default"><i class="glyphicon glyphicon-chevron-down"></i>&nbsp;Expand All</a>&nbsp;&nbsp;
                            <a id="btnAgrCollapseAllPanel" class="btn btn-sm btn-default"><i class="glyphicon glyphicon-chevron-up"></i>&nbsp;Collapse All</a>
                        </span>
                    </div>
                    <div id="accountDetailCP" class="panel-collapse collapse in">
                        @*  style="height:500px; overflow:auto" *@
                        <div class="panel panel-default container" style="width: 1200px;" ng-repeat="accountDetail in onBoardingAccountDetails track by $index" on-last-repeat id="Panel{{$index}}" plindex="{{$index}}">
                            <div class="panel-heading row" style="cursor: pointer;">
                                <h4 class="panel-title">
                                    <span class="control-label">{{accountDetail.AccountName}}</span>
                                    <span class="pull-right">
                                        <a class="btn btn-sm btn-danger" ng-click="fnRemoveAccountDetail(accountDetail)" style="display: none;"><i class="glyphicon glyphicon-trash"></i>&nbsp;Delete</a>
                                        &nbsp;&nbsp;<i class="glyphicon glyphicon-chevron-down"></i>
                                    </span>
                                </h4>
                            </div>
                            <div class="collapse">
                                <div class="panel-body" id="accountDetailDiv">
                                    <input type="hidden" ng-model="accountDetail.onBoardingAccountId" />
                                    <p></p>
                                    <div class="row control-group" style="margin-left: 15px; margin-right: 10px;">
                                        <legend class="small font-primary">Account Description:</legend>
                                    </div>
                                    <p></p>
                                    <div class="row">
                                        <div class="col-sm-12 col-md-9 col-lg-6 paddingleft">
                                            <div class="col-sm-8 col-md-4 col-lg-4">
                                                <label class="control-label">Account Name </label>
                                            </div>
                                            <div class="col-sm-8 col-md-5 col-lg-5">
                                                <input type="text" class="form-control" ng-model="accountDetail.AccountName" />
                                            </div>
                                        </div>
                                        <div class="col-sm-12 col-md-9 col-lg-6 paddingleft">
                                            <div class="col-sm-8 col-md-4 col-lg-4">
                                                <label class="control-label">Currency </label>
                                            </div>
                                            <div class="col-sm-8 col-md-5 col-lg-5 AdjustDropDownWidth">
                                                <input type="text" class="form-control" ng-model="accountDetail.Currency" id="liCurrency{{$index}}" ng-change="fnCutOffTime(accountDetail.Currency,accountDetail.CashInstruction, $index)" />
                                            </div>
                                            <div class="col-sm-8 col-md-3 col-lg-3">
                                                <a class="btn btn-sm btn-info" ng-click="fnAddCurrencyModal($index)" title="Add Currency"><i class="glyphicon glyphicon-plus"></i></a>
                                            </div>
                                        </div>
                                    </div>
                                    <p></p>
                                    <div class="row">
                                        @*<div class="col-sm-12 col-md-9 col-lg-6 paddingleft">
                <div class="col-sm-8 col-md-4 col-lg-4">
                    <label class="control-label">Account Type</label>
                </div>
                <div class="col-sm-8 col-md-5 col-lg-5 AdjustDropDownWidth">
                    <input type="text" class="form-control" ng-model="accountDetail.AccountPurpose" id="liAccountPurpose{{$index}}" />
                </div>
            </div>*@
                                        <div class="col-sm-12 col-md-9 col-lg-6 paddingleft">
                                            <div class="col-sm-8 col-md-4 col-lg-4">
                                                <label class="control-label">Account Status</label>
                                            </div>
                                            <div class="col-sm-8 col-md-5 col-lg-5 AdjustDropDownWidth">
                                                <input type="text" class="form-control" ng-model="accountDetail.AccountStatus" id="liAccountStatus{{$index}}" />
                                            </div>
                                        </div>
                                    </div>
                                    <p></p>
                                    <div class="row">
                                        <div class="col-sm-12 col-md-9 col-lg-6 paddingleft">
                                            <div class="col-sm-8 col-md-4 col-lg-4">
                                                <label class="control-label">Description </label>
                                            </div>
                                            <div class="col-sm-8 col-md-5 col-lg-5 AdjustDropDownWidth">
                                                <input ng-model="accountDetail.Description" class="form-control control-label" id="liAccountDescriptions{{$index}}">
                                            </div>
                                            <div class="col-sm-8 col-md-3 col-lg-3">
                                                <a id="btnAccountDescription" class="btn btn-sm btn-info" ng-click="fnAddAccountDetailModal($index, 'Description')" title="Add Description"><i class="glyphicon glyphicon-plus"></i></a>
                                            </div>
                                        </div>
                                        <div class="col-sm-12 col-md-9 col-lg-6 paddingleft">
                                            <div class="col-sm-8 col-md-4 col-lg-4">
                                                <label class="control-label">Notes </label>
                                            </div>
                                            <div class="col-sm-8 col-md-5 col-lg-5 AdjustDropDownWidth">
                                                <textarea cols="10" rows="3" ng-model="accountDetail.Notes" class="form-control control-label" placeholder="Notes"></textarea>
                                            </div>
                                        </div>
                                    </div>
                                    <p></p>
                                    <div class="row">
                                        <div class="col-sm-12 col-md-9 col-lg-6 paddingleft" ng-hide="accountDetail.IsReceivingAccount">
                                            <div class="col-sm-8 col-md-4 col-lg-4">
                                                <label class="control-label">Module </label>
                                            </div>
                                            <div class="col-sm-8 col-md-5 col-lg-5 AdjustDropDownWidth">
                                                <input ng-model="accountDetail.AccountModule" class="form-control control-label" id="liAccountModule_{{$index}}">
                                            </div>
                                            <div class="col-sm-8 col-md-3 col-lg-3">
                                                <a id="btnAccountModule" class="btn btn-sm btn-info" ng-click="fnAddAccountDetailModal($index, 'Module')" title="Add Module"><i class="glyphicon glyphicon-plus"></i></a>
                                            </div>
                                        </div>
                                        <div class="col-sm-12 col-md-9 col-lg-6 paddingleft">
                                            <div class="col-sm-8 col-md-4 col-lg-4">
                                                <label class="control-label">Account Number </label>
                                            </div>
                                            <div class="col-sm-8 col-md-5 col-lg-5">
                                                <input ng-model="accountDetail.ContactNumber" class="form-control control-label">
                                            </div>
                                        </div>
                                    </div>
                                    <p></p>
                                    <div class="row"ng-hide="accountDetail.IsReceivingAccount">
                                        <div class="col-sm-12 col-md-9 col-lg-6 paddingleft">
                                            <div class="col-sm-8 col-md-4 col-lg-4">
                                                <label class="control-label">Swift Group <span style="color: red">  *</span></label>
                                            </div>
                                            <div class="col-sm-8 col-md-5 col-lg-5 AdjustDropDownWidth">
                                                <input type="text" name="Swift Group" class="form-control" ng-model="accountDetail.SwiftGroup" id="liSwiftGroup{{$index}}" ng-change="fnOnSwiftGroupChange(accountDetail.SwiftGroup, $index)" required />
                                            </div>
                                            <div class="col-sm-8 col-md-3 col-lg-3">
                                                <a class="btn btn-sm btn-info" ng-click="fnAddSwiftGroupModal($index)" title="Add Swift Group"><i class="glyphicon glyphicon-plus"></i></a>
                                            </div>
                                        </div>
                                        <div class="col-sm-12 col-md-9 col-lg-6 paddingleft">
                                            <div class="col-sm-8 col-md-4 col-lg-4">
                                                <label class="control-label">Senders BIC </label>
                                            </div>
                                            <div class="col-sm-8 col-md-5 col-lg-5">
                                                <input type="text" class="form-control" ng-model="accountDetail.SendersBIC" id="txtSender{{$index}}" />
                                            </div>
                                        </div>
                                    </div>
                                    <p></p>
                                    <div class="row">
                                        <div class="col-sm-12 col-md-9 col-lg-6 paddingleft">
                                            <div class="col-sm-8 col-md-4 col-lg-4">
                                                <label class="control-label">Authorized Party</label>
                                            </div>
                                            <div class="col-sm-8 col-md-5 col-lg-5 AdjustDropDownWidth">
                                                <input ng-model="accountDetail.AuthorizedParty" class="form-control control-label" id="liAuthorizedParty{{$index}}" ng-change="fnAuthorizedPartyChange($index)">
                                            </div>
                                            <div class="col-sm-8 col-md-3 col-lg-3">
                                                <a class="btn btn-sm btn-info" ng-click="fnAddAuthorizedPartyModal($index)" title="Add Authorized Party"><i class="glyphicon glyphicon-plus"></i></a>
                                            </div>
                                        </div>
                                        <div class="col-sm-12 col-md-9 col-lg-6 paddingleft">
                                            <div class="col-sm-8 col-md-4 col-lg-4">
                                                <label class="control-label">Cash Instruction Mechanism</label>
                                            </div>
                                            <div class="col-sm-8 col-md-5 col-lg-5 AdjustDropDownWidth">
                                                <input type="text" class="form-control" ng-model="accountDetail.CashInstruction" id="liCashInstruction{{$index}}" ng-change="fnCutOffTime(accountDetail.Currency,accountDetail.CashInstruction, $index)" />
                                            </div>
                                            <div class="col-sm-8 col-md-3 col-lg-3">
                                                <a class="btn btn-sm btn-info" ng-click="fnAddCashInstructionModal($index)" title="Add Cash Instruction"><i class="glyphicon glyphicon-plus"></i></a>
                                            </div>
                                        </div>
                                    </div>
                                    <p></p>
                                    <div class="row">
                                        <div class="col-sm-12 col-md-9 col-lg-6 paddingleft" ng-hide="accountDetail.IsReceivingAccount" >
                                            <div class="col-sm-8 col-md-4 col-lg-4">
                                                <label class="control-label">Cash Sweep</label>
                                            </div>
                                            <div class="col-sm-8 col-md-5 col-lg-5 AdjustDropDownWidth">
                                                <input ng-model="accountDetail.CashSweep" class="form-control control-label" id="cashSweep{{$index}}" ng-change="fnCashSweep(accountDetail.CashSweep, $index)">
                                            </div>
                                        </div>
                                        <div class="col-sm-12 col-md-9 col-lg-6 paddingleft">
                                            <div class="col-sm-8 col-md-4 col-lg-4">
                                                <label class="control-label">Cutoff Time Zone</label>
                                            </div>
                                            <div class="col-sm-8 col-md-6 col-lg-7">
                                                <div class="col-sm-8 col-md-6 col-lg-6" style="padding-left: 0;">
                                                    <input type="text" name="cutoffTimeZone" ng-model="accountDetail.CutOffTimeZone" class="form-control" id="liTimeZone{{$index}}" disabled style="padding: 5px;" />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <p></p>
                                    <div class="row">
                                        <div class="col-sm-12 col-md-9 col-lg-6 paddingleft">
                                        </div>
                                        <div class="col-sm-12 col-md-9 col-lg-6 paddingleft">
                                            <div class="col-sm-8 col-md-4 col-lg-4">
                                                <label class="control-label">Wire Portal Cutoff</label>
                                            </div>
                                            <div class="col-sm-8 col-md-6 col-lg-7">
                                                <div class="col-sm-8 col-md-6 col-lg-6" style="padding-left: 0;">
                                                    <input type="time" name="cutOffTime" ng-model="accountDetail.CutoffTime" class="form-control" id="cutOffTime{{$index}}" disabled style="padding: 5px;" />
                                                </div>
                                                <div class="col-sm-8 col-md-4 col-lg-4 AdjustDropDownWidth" style="margin-top: 5px; padding-left: 0;">
                                                    <label class="label label-success" id="wireDays{{$index}}" style="font-size: 12px; padding: 5px;">{{accountDetail.DaystoWire}} {{accountDetail.DaystoWire > 1 ? "Days" : "Day"}}</label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>                                    
                                    <p></p>
                                    <div class="cashSweepTimeDiv cashSweepTimeDiv{{$index}}">
                                        <div class="row control-group" ng-hide="accountDetail.IsReceivingAccount" style="margin-left: 15px; margin-right: 10px;">
                                            <legend class="small font-primary">Cash Sweep:</legend>
                                        </div>
                                        <p></p>
                                        <div class="row">
                                            <div class="col-sm-12 col-md-9 col-lg-6 paddingleft" ng-hide="accountDetail.IsReceivingAccount">
                                                <div class="col-sm-8 col-md-4 col-lg-4">
                                                    <label class="control-label">Cash Sweep Time</label>
                                                </div>
                                                <div class="col-sm-8 col-md-7 col-lg-7">
                                                    <div class="col-sm-8 col-md-5 col-lg-5" style="padding-left: 0;">
                                                        <input type="time" name="Cash Sweep Time" ng-model="accountDetail.CashSweepTime" class="form-control" id="cashSweepTime{{$index}}" ng-required="accountDetail.CashSweep == 'Yes'" style="padding: 2px;" />
                                                    </div>
                                                    <div class="col-sm-8 col-md-4 col-lg-4 AdjustDropDownWidth" style="padding-left: 0; padding-right: 20px;">
                                                        <input name="Cash Sweep Time Zone" ng-model="accountDetail.CashSweepTimeZone" class="form-control control-label" id="cashSweepTimeZone{{$index}}" ng-required="accountDetail.CashSweep == 'Yes'">
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-sm-12 col-md-9 col-lg-6 paddingleft">
                                                <div class="col-sm-8 col-md-4 col-lg-4">
                                                    <label class="control-label">Holdback Amount</label>
                                                </div>
                                                <div class="col-sm-8 col-md-5 col-lg-5">
                                                    <input type="text" class="form-control" ng-model="accountDetail.HoldbackAmount" />
                                                </div>
                                            </div>
                                        </div>
                                        <p></p>
                                        <div class="row">
                                            <div class="col-sm-12 col-md-9 col-lg-6 paddingleft">
                                                <div class="col-sm-8 col-md-4 col-lg-4">
                                                    <label class="control-label">Ticker/ISIN</label>
                                                </div>
                                                <div class="col-sm-8 col-md-5 col-lg-5">
                                                    <input type="text" class="form-control" ng-model="accountDetail.TickerorISIN" />
                                                </div>
                                            </div>
                                            <div class="col-sm-12 col-md-9 col-lg-6 paddingleft">
                                                <div class="col-sm-8 col-md-4 col-lg-4">
                                                    <label class="control-label">Associated Custody Account</label>
                                                </div>
                                                <div class="col-sm-8 col-md-5 col-lg-5 AdjustDropDownWidth">
                                                    <input type="text" class="form-control" ng-model="accountDetail.AssociatedCustodyAcct" id="liCustodyAcct{{$index}}" />
                                                </div>
                                            </div>
                                        </div>
                                        <p></p>
                                        <div class="row">
                                            <div class="col-sm-12 col-md-9 col-lg-6 paddingleft">
                                                <div class="col-sm-8 col-md-4 col-lg-4">
                                                    <label class="control-label">Portfolio Selection</label>
                                                </div>
                                                <div class="col-sm-8 col-md-5 col-lg-5">
                                                    <input type="text" class="form-control" ng-model="accountDetail.PortfolioSelection" />
                                                </div>
                                            </div>
                                            <div class="col-sm-12 col-md-9 col-lg-6 paddingleft">
                                                <div class="col-sm-8 col-md-4 col-lg-4">
                                                    <label class="control-label">Currency</label>
                                                </div>
                                                <div class="col-sm-8 col-md-5 col-lg-5 AdjustDropDownWidth">
                                                    <input type="text" class="form-control" id="liSweepCurrency{{$index}}" ng-model="accountDetail.SweepCurrency" />
                                                </div>
                                            </div>
                                        </div>
                                        <p></p>
                                        <div class="row">
                                            <div class="col-sm-12 col-md-9 col-lg-6 paddingleft">
                                                <div class="col-sm-8 col-md-4 col-lg-4">
                                                    <label class="control-label">Sweep comments</label>
                                                </div>
                                                <div class="col-sm-8 col-md-5 col-lg-5">
                                                    <textarea cols="10" rows="3" ng-model="accountDetail.SweepComments" class="form-control control-label" placeholder="Sweep Comments"></textarea>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row control-group" style="margin-left: 15px; margin-right: 10px;">
                                        <legend class="small font-primary">Additional Details:</legend>
                                    </div>
                                    <p></p>
                                    <div class="row">
                                        <div class="col-sm-12 col-md-9 col-lg-6 paddingleft">
                                            <div class="col-sm-8 col-md-4 col-lg-4">
                                                <label class="control-label">FFC Name</label>
                                            </div>
                                            <div class="col-sm-8 col-md-5 col-lg-5">
                                                <input type="text" name='FFCName' ng-pattern="/^[/?:().,'+-\w\s\n\r]*$/" ng-model="accountDetail.FFCName" class="form-control control-label" placeholder="FFC Name" />
                                            </div>
                                        </div>
                                        <div class="col-sm-12 col-md-9 col-lg-6 paddingleft">
                                            <div class="col-sm-8 col-md-4 col-lg-4">
                                                <label class="control-label">FFC Number </label>
                                            </div>
                                            <div class="col-sm-8 col-md-5 col-lg-5">
                                                <input type="text" name='FFCNumber' ng-pattern="/^[/?:().,'+-\w\s\n\r]*$/" ng-model="accountDetail.FFCNumber" class="form-control control-label" placeholder="FFC Number" />
                                            </div>
                                        </div>
                                    </div>
                                    <p></p>
                                    <div class="row">
                                        <div class="col-sm-12 col-md-9 col-lg-6 paddingleft">
                                            <div class="col-sm-8 col-md-4 col-lg-4">
                                                <label class="control-label">Reference </label>
                                            </div>
                                            <div class="col-sm-8 col-md-5 col-lg-5">
                                                <input type="text" name='Reference' ng-pattern="/^[/?:().,'+-\w\s\n\r]*$/" ng-model="accountDetail.Reference" class="form-control control-label" placeholder="Reference" />
                                            </div>
                                        </div>
                                    </div>
                                    <p></p>
                                    <div class="row">
                                        <div class="col-sm-6 col-md-3 col-lg-2 paddingleft">
                                        </div>
                                        <div class="col-sm-6 col-md-3 col-lg-3 paddingleft">
                                            <span class="row small font-primary">Intermediary</span>
                                        </div>
                                        <div class="col-sm-6 col-md-3 col-lg-3 paddingleft">
                                            <span class="row small font-primary">Beneficiary <span style="color: red">  *</span></span>
                                        </div>
                                        <div class="col-sm-6 col-md-3 col-lg-3 paddingleft">
                                            <span class="row small font-primary">Ultimate Beneficiary <span style="color: red">  *</span></span>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-6 col-md-3 col-lg-2 paddingleft">
                                            <label class="control-label" style="margin-left: 15px;">Account Number</label>
                                        </div>
                                        <div class="col-sm-6 col-md-3 col-lg-3 paddingleft">
                                            <input type="text" class="form-control" ng-model="accountDetail.IntermediaryAccountNumber" placeholder="Account Number" />
                                        </div>
                                        <div class="col-sm-6 col-md-3 col-lg-3 paddingleft">
                                            <input type="text" ng-model="accountDetail.BeneficiaryAccountNumber" class="form-control control-label" placeholder="Account Number" />
                                        </div>
                                        <div class="col-sm-6 col-md-3 col-lg-3 paddingleft">
                                            <input name="Account Number" type="text" class="form-control" ng-model="accountDetail.AccountNumber" placeholder="Account Number" required />
                                        </div>
                                    </div>
                                    <p></p>

                                    <div class="row">
                                        <div class="col-sm-6 col-md-3 col-lg-2 paddingleft">
                                            <label for="liBeneficiaryType{{$index}}" class="control-label" style="margin-left: 15px;">Type:</label>
                                        </div>
                                        <div class="col-sm-6 col-md-3 col-lg-3 paddingleft AdjustDropDownWidth">
                                            <input type="text" class="form-control" ng-model="accountDetail.IntermediaryType" id="liIntermediaryType{{$index}}" ng-change="fnToggleBeneficiaryBICorABA(accountDetail.IntermediaryType,'Intermediary',$index)" />
                                        </div>
                                        <div class="col-sm-6 col-md-3 col-lg-3 paddingleft AdjustDropDownWidth">
                                            <input name="Beneficiary Type" type="text" class="form-control" ng-model="accountDetail.BeneficiaryType" id="liBeneficiaryType{{$index}}" ng-change="fnToggleBeneficiaryBICorABA(accountDetail.BeneficiaryType,'Beneficiary',$index)" required />
                                        </div>
                                        <div class="col-sm-6 col-md-3 col-lg-3 paddingleft AdjustDropDownWidth">
                                            <input type="text" class="form-control" ng-model="accountDetail.UltimateBeneficiaryType" id="liUltimateBeneficiaryType{{$index}}" ng-change="fnToggleBeneficiaryBICorABA(accountDetail.UltimateBeneficiaryType,'UltimateBeneficiary',$index)" />
                                        </div>
                                    </div>
                                    <p></p>

                                    <div class="row">
                                        <div class="col-sm-6 col-md-3 col-lg-2 paddingleft">
